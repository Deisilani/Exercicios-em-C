#include <stdio.h>
#include <stdlib.h>

int main() {

  int n1, n2, n3, m;

  printf("Insira o primeiro número\n\n");
  scanf("%d", &n1);

  printf("Insira o segundo número\n\n");
  scanf("%d", &n2);

  printf("Insira o terceiro número\n\n");
  scanf("%d", &n3);

  if (n1 > n2 && n1 > n3){
      m = n1;
  }
  
  if(n2 > n3){
      m = n2;
  }
          
  else{
      m = n3;
  }
  printf("O maior número é: %d\n",m);
  return 0;
}
